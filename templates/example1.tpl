<PCPIN:TPL name="main">
  <html>
    <head>
      <title>PCPIN Template usage examples</title>
    </head>
    <body>
      <b><u>Example 1: Setting variables</u></b>
      <br /><br />
      <table border="1" cellspacing="0" cellpadding="10">
        <tr>
          <td align="left">Current date is:</td>
          <td align="center">{DATE}</td>
        </tr>
        <tr>
          <td align="left">Current time is:</td>
          <td align="center">{TIME}</td>
        </tr>
        <tr>
          <td align="left">Random number between 0 and 1000:</td>
          <td align="center">{RANDOM}</td>
        </tr>
        <tr>
          <td align="left">Your IP address:</td>
          <td align="center">{IP}</td>
        </tr>
      </table>
      <br />
      <a href="examples.html">Return to the main page</a>
    </body>
  </html>
</PCPIN:TPL>